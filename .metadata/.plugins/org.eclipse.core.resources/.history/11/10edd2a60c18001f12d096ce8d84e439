package com.techlabs.model;

public class Rectangle {
	private int height;
	private int width;
	private int area;
	
	public void setHeight(int height1) {
		height=height1;
	}
	public void setWidth(int width1) {
		width=width1;
	}
	public void setArea(int area1) {
		area1=height*width;
		area=area1;
	}
	
	public int getHeight() {
		return height;
	}
	public int getWidth() {
		return width;
	}
	public int getArea(){
		return area;
	}
}
